<%= form_for(@auth_infor) do |f| %>
  <div class="row">
    <div class="form-group">
      <div>
        <%= f.label :client_id, 'Client' %>
      </div>
      <div class="col-lg-12">
        <%= f.text_field :client_id, value: @auth_infor.client_id, visiable: false  %>
      </div>
    </div>
    <div class="form-group">
       <div>
        <%= f.label :login_url, 'Login Url' %>
      </div>
      <div class="col-lg-12">
        <%= f.text_field :login_url, value: @auth_infor.login_url %>
      </div>
    </div>
    <div class="form-group">
       <div>
        <%= f.label :user_name, 'User Name' %>
      </div>
      <div class="col-lg-12">
        <%= f.text_field :user_name, value: @auth_infor.user_name %>
      </div>
    </div>
    <div class="form-group">
       <div>
        <%= f.label :password, 'Password' %>
      </div>
      <div class="col-lg-12">
        <%= f.text_field :password, value: @auth_infor.password %>
      </div>
    </div>
    <div class="form-group">
      <%= f.submit 'Save' %>
    </div>
  </div>
<% end %>